pipeline {
    agent {
        docker {
            image 'goforgold/build-container:latest'
        }
    }
    stages {
        stage('Install dependencies') {
            steps {
                sh 'mkdir -p terraform'
                dir('terraform') {
                    git url: 'https://github.com/AtlasBID/CalibrationResults.git'
                    sh 'ls'
                }
            }
        }
}